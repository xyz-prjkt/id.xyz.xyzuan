    android:id="@+id/xyz_me"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/GraphiteBlack"
    tools:context=".AboutMe">

    <LinearLayout
        android:id="@+id/frame_container"
        android:layout_width="match_parent"
